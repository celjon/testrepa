    "start:dev": "cross-env NODE_ENV=development dotenv -e .env -- nodemon --watch \"./src/**/*\" --watch \"./config/config.yml\" --signal SIGTERM --exec tsx ./src/app.ts",
    "start:prod": "cross-env NODE_ENV=production node build/src/app.js",
    "build:prod": "bunx swc src -d build",
    "migrate:dev": "dotenv -e .env npx prisma db push",
    "migrate:prod": "npx prisma db push --accept-data-loss",
    "seed:dev": "dotenv -e .env -- tsx ./db/seed.ts",
    "seed:prod": "tsx ./db/seed.ts",
    "reset:dev": "dotenv -e .env npx prisma migrate reset",
    "studio:dev": "dotenv -e .env -- npx prisma studio",
    "test": "jest",
    "lint": "eslint -c eslint.config.js ./src --fix",
    "commitlint": "commitlint --edit",
    "prepare": "husky || true"
  },
